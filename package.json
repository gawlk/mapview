{
  "name": "mapview",
  "description": "A web application that generates reports from Rincent ND Technologies machines data files",
  "version": "2.1.10",
  "homepage": "https://v2.mapview.fr/",
  "license": "gpl",
  "scripts": {
    "dev": "($npm_execpath outdated || read -p \"Press enter to ignore...\") && vite --host",
    "check": "tsc --noEmit --skipLibCheck",
    "build": "(command -v zip && cd src/assets/templates && rm -f templates.zip && zip -r templates.zip . && cd -) || true && $npm_execpath run check && vite build && sed 's+media=\"(device+media=\"screen and (device+g' dist/index.html | sed 's+</head>+<meta name=\"apple-touch-fullscreen\" content=\"yes\" /></head>+g' > index.html.tmp && mv index.html.tmp dist/index.html",
    "prod": "$npm_execpath run build && vite preview --host",
    "serve": "vite preview",
    "test": "sed -i.bak 's|DEV$1\\.|DEV$1?\\.|g' node_modules/.pnpm/solid-js@*/node_modules/solid-js/store/dist/dev.js && sed -i.bak 's|\"./integration\"|\"./integration.js\"|g' node_modules/.pnpm/@solidjs+router@*_solid-js@*/node_modules/@solidjs/router/dist/routing.js && sed -i.bak 's|\"./lifecycle\"|\"./lifecycle.js\"|g' node_modules/.pnpm/@solidjs+router@*_solid-js@*/node_modules/@solidjs/router/dist/routing.js && sed -i.bak 's|\"./utils\"|\"./utils.js\"|g' node_modules/.pnpm/@solidjs+router@*_solid-js@*/node_modules/@solidjs/router/dist/routing.js && TZ=Europe/Paris vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "test-coverage": "vitest --coverage",
    "test-browser": "playwright test --browser=all",
    "format": "prettier --write './src'",
    "check-format": "prettier --check './src'",
    "prepare": "husky install",
    "lint": "pnpm eslint ./src"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@gawlk/draggable": "1.0.0-beta.15",
    "@shopify/draggable": "1.0.0-beta.12",
    "@solid-primitives/event-listener": "^2.3.0",
    "@solid-primitives/i18n": "^1.4.1",
    "@solid-primitives/intersection-observer": "^2.1.3",
    "@solid-primitives/map": "^0.4.8",
    "@solid-primitives/media": "^2.2.5",
    "@solid-primitives/mouse": "^2.0.16",
    "@solid-primitives/resize-observer": "^2.0.22",
    "@solid-primitives/scroll": "^2.0.20",
    "@solid-primitives/timer": "^1.3.7",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@thisbeyond/solid-dnd": "^0.7.4",
    "dayjs": "^1.11.9",
    "dedent": "^1.5.1",
    "dragula": "^3.7.3",
    "dragula-fixed": "^3.7.5",
    "fflate": "^0.8.1",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "iconoir": "^6.11.0",
    "localforage": "^1.10.0",
    "mapbox-gl": "2.15.0",
    "mathjs": "^11.11.0",
    "pica": "^9.0.1",
    "solid-js": "^1.7.12",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@darkobits/vite-plugin-favicons": "^0.3.0",
    "@edge-runtime/vm": "^3.1.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@iconify-json/heroicons-solid": "^1.1.8",
    "@iconify-json/tabler": "^1.1.91",
    "@playwright/test": "^1.38.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/dedent": "^0.7.0",
    "@types/dragula": "^3.7.1",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^2.7.14",
    "@types/node": "^20.6.2",
    "@types/pica": "^9.0.1",
    "@types/sortablejs": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^11.2.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unimport": "^3.3.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "vite": "^4.4.9",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-solid-svg": "^0.6.4",
    "vitest": "^0.34.4"
  }
}
