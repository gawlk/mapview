{
  "name": "mapview",
  "description": "A web application that generates reports from Rincent ND Technologies machines data files",
  "version": "2.1.9",
  "homepage": "https://v2.mapview.fr/",
  "license": "gpl",
  "scripts": {
    "dev": "($npm_execpath outdated || read -p \"Press enter to ignore...\") && vite --host",
    "check": "tsc --noEmit --skipLibCheck",
    "build": "cd src/assets/templates && rm -f templates.zip && zip -r templates.zip . && cd - && $npm_execpath run check && vite build",
    "prod": "$npm_execpath run build && vite preview --host",
    "serve": "vite preview",
    "test": "sed -i.bak 's|\"./integration\"|\"./integration.js\"|g' node_modules/.pnpm/@solidjs+router@*_solid-js@*/node_modules/@solidjs/router/dist/routing.js && sed -i.bak 's|\"./lifecycle\"|\"./lifecycle.js\"|g' node_modules/.pnpm/@solidjs+router@*_solid-js@*/node_modules/@solidjs/router/dist/routing.js && sed -i.bak 's|\"./utils\"|\"./utils.js\"|g' node_modules/.pnpm/@solidjs+router@*_solid-js@*/node_modules/@solidjs/router/dist/routing.js && TZ=Europe/Paris vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "test-coverage": "vitest --coverage",
    "test-browser": "playwright test --browser=all",
    "format": "prettier --write './src'",
    "check-format": "prettier --check './src'",
    "prepare": "husky install",
    "lint": "pnpm eslint ./src"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.1",
    "@solid-primitives/event-listener": "^2.2.14",
    "@solid-primitives/i18n": "^1.4.1",
    "@solid-primitives/intersection-observer": "^2.1.3",
    "@solid-primitives/map": "^0.4.7",
    "@solid-primitives/media": "^2.2.4",
    "@solid-primitives/mouse": "^2.0.15",
    "@solid-primitives/resize-observer": "^2.0.20",
    "@solid-primitives/scroll": "^2.0.19",
    "@solid-primitives/timer": "^1.3.7",
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@thisbeyond/solid-dnd": "^0.7.4",
    "dayjs": "^1.11.9",
    "dedent": "^1.5.1",
    "fflate": "^0.8.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "iconoir": "^6.11.0",
    "localforage": "^1.10.0",
    "mapbox-gl": "2.15.0",
    "mathjs": "^11.10.0",
    "pica": "^9.0.1",
    "solid-js": "^1.7.11"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^3.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@iconify-json/heroicons-solid": "^1.1.8",
    "@iconify-json/tabler": "^1.1.89",
    "@playwright/test": "^1.37.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/dedent": "^0.7.0",
    "@types/geojson": "^7946.0.10",
    "@types/mapbox-gl": "^2.7.13",
    "@types/node": "^20.5.7",
    "@types/pica": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@vitest/coverage-v8": "^0.34.3",
    "@vitest/ui": "^0.34.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "happy-dom": "^10.11.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.28",
    "prettier": "3.0.2",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unimport": "^3.2.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.6",
    "vite": "^4.4.9",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-solid-svg": "^0.6.4",
    "vitest": "^0.34.3"
  }
}
