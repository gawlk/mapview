{
  "name": "mapview",
  "description": "A web application that generates reports from Rincent ND Technologies machines data files",
  "version": "2.1.2",
  "homepage": "https://v2.mapview.fr/",
  "license": "gpl",
  "scripts": {
    "dev": "vite --host",
    "check": "vue-tsc --noEmit --skipLibCheck",
    "build": "$npm_execpath run check && vite build",
    "prod": "$npm_execpath run build && vite preview --host",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "test-coverage": "vitest --coverage",
    "prettier": "prettier --write './src'",
    "prepare": "husky install"
  },
